{
  "moduleFileExtensions": ["js", "json", "ts"],
  "testRegex": ".*\\.spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": ["**/*.(t|j)s"],
  "coverageDirectory": "<rootDir>/coverage/",
  "testEnvironment": "node",
    "resetMocks": true,
    "clearMocks": true
}
